<div class="thewholeshebang">
<div class="cssNJ">
  <h2>Welcome, {{name}}!</h2>
  <h3>Create a New Joke</h3>
  </div>
  <div class="newjokegroup">
    <form>
      <div class="form-group">
        <label for="joke-name">Joke Question:</label>
        <input class="form-input" type="text" id="joke-name" name="joke-name" />
      </div>
      <div class="form-group">
        <label for="joke-desc">Punch Line:</label>
        <textarea class="form-input" id="joke-desc" name="joke-desc"></textarea>
      </div>
      <div class="form-group">
        <button type="submit" class="btn btn-primary">Create</button>
      </div>
    </form>
  </div>
  {{#if jokes.length}}
    <h3>Current jokes:</h3>

    {{#each jokes as |joke|}}
    <h4><a href="/jokes/{{joke.jokeId}}">{{joke.jokeSetUp}}</a></h4>
    <div class="col-md-4">
      <button class="btn btn-sm btn-danger" data-id="{{joke.jokeId}}">DELETE</button>
  </div>
  {{/each}}
</div>
{{/if}}
</div>
</div>
<script src="/js/joke.js"></script> 
